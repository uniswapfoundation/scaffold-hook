    "genesis": "source .env && forge script contracts/script/GenerateAnvilGenesis.s.sol --code-size-limit 30000 --ffi --broadcast --rpc-url http://127.0.0.1:8545 --private-key 0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80",
